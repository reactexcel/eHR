  "scripts": {
    "start:dev": "better-npm-run start:development",
    "deploy:dev": "better-npm-run deploy:development",
    "start:prod": "better-npm-run start:production",
    "deploy:prod": "better-npm-run deploy:production",
    "start": "npm run start:dev",
    "eslint": "./node_modules/eslint/bin/eslint.js --fix src"
  },
  "betterScripts": {
    "start:development": {
      "command": "babel-node bin/server",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "start:production": {
      "command": "babel-node bin/server",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "deploy:development": {
      "command": "rimraf dist && babel-node bin/compile",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "deploy:production": {
      "command": "rimraf dist && babel-node bin/compile",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
